{
  "applicationId": "hmm.curtain",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "window.*"
  ],
  "logging": {
    "writeToLogFile": false
  },
  "modes": {
    "window": {
      "title": "hedgemodmanager-curtain",
      "width": 1280,
      "height": 720,
      "minWidth": 1280,
      "minHeight": 720,
      "fullScreen": true,
      "icon": "/assets/icon.png",
      "enableInspector": false,
      "borderless": true,
      "center": true,
      "resizable": false,
      "exitProcessOnClose": true,
      "transparent": true,
      "useSavedState": false
    }
  },
  "cli": {
    "binaryName": "hedgemodmanager-curtain",
    "resourcesPath": "/build",
    "binaryVersion": "5.4.0",
    "clientVersion": "3.12.0"
  }
}