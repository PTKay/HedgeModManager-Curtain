{
  "applicationId": "hmm.curtain",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "logging": {
    "writeToLogFile": false
  },
  "modes": {
    "window": {
      "title": "hedgemodmanager-curtain",
      "width": 853,
      "height": 480,
      "minWidth": 853,
      "minHeight": 480,
      "fullScreen": true,
      "icon": "/assets/icon.png",
      "enableInspector": false,
      "borderless": true,
      "center": true,
      "resizable": false,
      "exitProcessOnClose": false,
      "transparent": true
    }
  },
  "cli": {
    "binaryName": "hedgemodmanager-curtain",
    "resourcesPath": "/build",
    "binaryVersion": "5.3.0",
    "clientVersion": "3.12.0"
  }
}